{
  "name": "Quantum Pi Forge Agent Environment",
  "version": "1.0.0",
  "agent_framework": "agent-framework",
  "capabilities": {
    "deploy": {
      "script": ".agent/scripts/deploy.sh",
      "timeout_seconds": 3600,
      "requires_approval": false
    },
    "rollback": {
      "script": ".agent/scripts/rollback.sh",
      "timeout_seconds": 900,
      "requires_approval": true
    },
    "validate": {
      "script": ".agent/scripts/validate.sh",
      "timeout_seconds": 1800,
      "requires_approval": false
    },
    "healthcheck": {
      "script": ".agent/scripts/healthcheck.sh",
      "timeout_seconds": 300,
      "requires_approval": false
    }
  },
  "environment_variables": {
    "GITHUB_TOKEN": "from-secrets",
    "AGENT_NAME": "quantum-pi-forge",
    "AGENT_VERSION": "1.0.0"
  },
  "webhook_events": ["push", "pull_request", "workflow_dispatch"],
  "dispatch_url": ".github/workflows/agent_dispatch.yml"
}
